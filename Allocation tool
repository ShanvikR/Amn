Range("AC2", "AC" & lastrow).Formula = "=IFERROR(VLOOKUP(G2,Hidden!$DM:$EX,9,0),"""")"
Range("AD2", "AD" & lastrow).Formula = "=IFERROR(VLOOKUP(G2,Hidden!FQ:FS,3,0),"""")"
Range("A2", "AD" & lastrow) = Range("A2", "AD" & lastrow).Value

sHostName = Environ$("username")
